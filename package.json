{
  "name": "nasa-media-explorer",
  "version": "1.0.0",
  "description": "A React application for exploring NASA's multimedia library",
  "author": "Steve Meredith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Steveeeie/nasa-media-explorer-vite"
  },
  "homepage": "https://github.com/Steveeeie/nasa-media-explorer-vite",
  "keywords": [
    "nasa",
    "react",
    "vite",
    "typescript",
    "tanstack",
    "three.js"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc -b && vite build && cp dist/index.html dist/404.html",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --browser.headless",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --browser.headless",
    "test:report": "open coverage/index.html"
  },
  "dependencies": {
    "@eulersoft/classnames": "^1.0.1",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-router": "^1.144.0",
    "motion": "^12.23.26",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.182.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tanstack/router-plugin": "^1.145.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser": "^4.0.16",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "msw": "^2.12.7",
    "playwright": "^1.57.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16",
    "vitest-browser-react": "^2.0.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
